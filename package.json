{
  "name": "next-auth-psql-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev | pino-pretty",
    "dev:3001": "cross-env PORT=3001 next dev -p 3001 | pino-pretty",
    "dev:test": "mkdir -p logs && cross-env NODE_ENV=test ALLOW_TEST_ENDPOINTS=true NEXT_PUBLIC_IS_E2E_TEST_ENV=true RATE_LIMIT_REGISTER_MAX_ATTEMPTS=1000 next dev -p 3777 | tee ./logs/e2e-server.log",
    "dev:test:debug": "NODE_OPTIONS='--inspect' npm run dev:test",
    "build": "next build",
    "start": "next start",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint .",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint --fix .",
    "lint:files": "ESLINT_USE_FLAT_CONFIG=true eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:all": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json,css,html}\"",
    "dev:clean": "npm run format && npm run lint:fix && npm run dev",
    "build:clean": "npm run format && npm run lint:fix && npm run build",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build:analyze": "ANALYZE=true next build",
    "validate": "npm run lint && npm run format:check && npm run type-check",
    "prepare": "husky",
    "db:seed": "npx prisma db seed",
    "test": "npm run test:unit",
    "test:ci": "npm run test:unit && npm run test:e2e",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:unit": "cross-env NODE_ENV=test jest --passWithNoTests --coverage",
    "test:unit:watch": "cross-env NODE_ENV=test jest --watch",
    "test:unit:subset": "cross-env NODE_ENV=test jest --findRelatedTests",
    "test:unit:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:unit:coverage:subset": "cross-env NODE_ENV=test JEST_SUBSET_COVERAGE=true jest --coverage --findRelatedTests",
    "test:e2e": "cross-env DISABLE_CLIENT_LOGGER_FETCH=true RATE_LIMIT_REGISTER_MAX_ATTEMPTS=1000 npx kill-port 8080 9099 4000 4400 4500 3777 &> /dev/null || true && rm -f ./logs/e2e-server.log && npx playwright test && node scripts/check-server-logs.js ./logs/e2e-server.log",
    "test:e2e:auth-only": "cross-env DISABLE_CLIENT_LOGGER_FETCH=true npx kill-port 8080 9099 4000 4400 4500 3777 &> /dev/null || true && rm -f ./logs/e2e-server.log && npx playwright test tests/e2e/authenticated/auth/ tests/e2e/public/auth/ && node scripts/check-server-logs.js ./logs/e2e-server.log",
    "test:e2e:public-only": "cross-env DISABLE_CLIENT_LOGGER_FETCH=true npx kill-port 8080 9099 4000 4400 4500 3777 &> /dev/null || true && rm -f ./logs/e2e-server.log && npx playwright test tests/e2e/public/ && node scripts/check-server-logs.js ./logs/e2e-server.log",
    "test:e2e:authenticated-only": "cross-env DISABLE_CLIENT_LOGGER_FETCH=true npx kill-port 8080 9099 4000 4400 4500 3777 &> /dev/null || true && rm -f ./logs/e2e-server.log && npx playwright test tests/e2e/authenticated/ && node scripts/check-server-logs.js ./logs/e2e-server.log",
    "test:e2e:help": "npx playwright test --help",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:update-snapshots": "playwright test --update-snapshots",
    "test:e2e:ci": "cross-env CI=true npm run test:e2e",
    "test:e2e:against-existing": "cross-env DISABLE_CLIENT_LOGGER_FETCH=true USE_EXISTING_SERVER=true PLAYWRIGHT_TEST_BASE_URL=http://localhost:3777 playwright test",
    "test:e2e:custom-url": "cross-env DISABLE_CLIENT_LOGGER_FETCH=true PLAYWRIGHT_TEST_BASE_URL=http://localhost:3000 playwright test",
    "ai:start": "npm run ai:port && npm run ai:verify-port && pm2 start ecosystem.config.js --env development && sleep 5",
    "ai:start:test": "npm run ai:port && npm run ai:verify-port && pm2 start ecosystem.config.js --env test && sleep 5",
    "ai:stop": "pm2 stop next-dev",
    "ai:restart": "npm run ai:port && npm run ai:verify-port && pm2 restart next-dev --update-env && sleep 5 && npm run ai:health",
    "ai:restart:test": "npm run ai:port && npm run ai:verify-port && pm2 restart next-dev --env test --update-env && sleep 5 && npm run ai:health",
    "ai:verify-port": "node scripts/verify-port.js",
    "ai:status": "pm2 status next-dev",
    "ai:logs": "pm2 logs next-dev --lines 100 --nostream",
    "ai:port": "node scripts/get-port.js",
    "ai:health": "node scripts/check-health.js",
    "dev:simple": "next dev -p 3336",
    "dev:background": "npm run dev &"
  },
  "dependencies": {
    "@auth/core": "^0.39.1",
    "@auth/prisma-adapter": "^2.8.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^7.0.2",
    "@mui/material-nextjs": "^7.0.2",
    "@mui/x-date-pickers": "^8.5.0",
    "@prisma/client": "^6.6.0",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.13",
    "ioredis": "^5.6.1",
    "next": "^15.3.0",
    "next-auth": "^5.0.0-beta.28",
    "next-themes": "^0.4.6",
    "pino": "^9.6.0",
    "rate-limiter-flexible": "^7.1.0",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.5.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@faker-js/faker": "^9.7.0",
    "@jest/globals": "^29.7.0",
    "@mui/material": "^7.0.2",
    "@next/bundle-analyzer": "^15.3.0",
    "@next/eslint-plugin-next": "^15.3.0",
    "@playwright/test": "^1.51.1",
    "@swc/core": "^1.12.1",
    "@swc/jest": "^0.2.38",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "async-lock": "^1.4.1",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "inquirer": "^12.5.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^6.0.0",
    "jest-mock": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "next-router-mock": "^1.0.2",
    "node-mocks-http": "^1.17.2",
    "pino-pretty": "^13.0.0",
    "playwright-core": "^1.52.0",
    "pm2": "^6.0.5",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.16.0",
    "server-only": "^0.0.1",
    "typescript": "^5.8.3"
  },
  "description": "A Next.js application with NextAuth.js and PostgreSQL",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/next-auth-psql-app"
  },
  "overrides": {
    "whatwg-url": "^14.0.0",
    "tr46": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "last 2 versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
